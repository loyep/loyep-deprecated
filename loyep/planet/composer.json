{
    "name": "loyep/planet",
    "description": "A web site framework base laravel. ",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Loren Liu",
            "email": "mwl@live.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Loyep\\Planet\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Loyep\\Planet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Loyep\\Planet\\PlanetServiceProvider"
            ],
            "aliases": {
                "Planet": "Loyep\\Planet\\Facades\\Planet"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
